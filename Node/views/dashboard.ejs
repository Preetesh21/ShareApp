<h1 class="mt-4">Dashboard</h1>
<p class="lead mb-3">Welcome
    <%= name %>
</p>
<% if(stories){ %>
    <% for(var i=0; i < stories.length; i++) { %>
        <p>
            <%=stories[i].title%>
        </p>
        <%=stories[i].body%>
            <p> by
                <%=stories[i].user.name %>
            </p>
            <% } %>
                <%}else { %>
                    <p>No stories to show</p>
                    <%}%>
                        <a href="/posts/add" class="btn btn-primary">Add</a>
                        <a href="/users/logout" class="btn btn-secondary">Logout</a>